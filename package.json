{
  "name": "Nuxt-TS-boilerplate-with-testing",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test:watch": "jest --env=jsdom --verbose ./test/unit",
    "test:unit": "jest --env=jsdom --verbose ./test/unit",
    "test:e2e": "jest --env=jest-environment-puppeteer --verbose ./test/e2e"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.15.1",
    "gsap": "^3.7.1",
    "nuxt": "^2.15.7",
    "nuxt-gsap-module": "^1.3.2",
    "nuxt-property-decorator": "^2.9.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.0",
    "@types/expect-puppeteer": "^4.4.6",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/puppeteer": "^5.4.4",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "fibers": "^5.0.0",
    "jest": "^27.0.5",
    "jest-environment-puppeteer": "^5.0.4",
    "jest-puppeteer": "^5.0.4",
    "node-sass": "^6.0.1",
    "prettier": "^2.3.2",
    "puppeteer": "^10.1.0",
    "sass": "^1.37.0",
    "sass-loader": "10.1.1",
    "ts-jest": "^27.0.3",
    "vue-jest": "^3.0.4"
  }
}
